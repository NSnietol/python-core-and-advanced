cadena = " Awesome "


paraffin= """
You are the creator
of your destiny
"""
print(cadena)

print(paraffin)

print(cadena*3)

print(cadena[1:])
print(cadena[2:6:1])

print("opposite")
print(cadena[::-1])
print("remove spaces")
print(cadena.strip())
print("Find index awe")
print(cadena.find("awe"))
print("Count")
print(cadena.count("a"))

