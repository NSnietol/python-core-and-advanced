from astropy._erfa.core import cal2jd
print("Int values")
a=12
b=231
c=2323
print(a,b,c)

print("Float point")

x=1.23
y=23.01
print(x,y)

print("Complex")
d = 3+ 2j
print(d)

print("Type conversion")

print("Binary ",bin(10))
print("Float",float("2.34"))
print("Hex",hex(3))